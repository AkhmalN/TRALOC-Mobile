{
  "expo": {
    "name": "unaspatrol",
    "slug": "unaspatrol",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/LOGOICON.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/LOGO.png"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos"
      },
      "bundleIdentifier": "com.akhmaln.unaspatrol"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.FOREGROUND_SERVICE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET",
        "android.permission.CAMERA"
      ],
      "package": "com.akhmaln.unaspatrol"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-camera",
      "expo-media-library",
      "expo-location",
      "expo-notifications"
    ],
    "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera.",
    "savePhotosPermission": "Allow $(PRODUCT_NAME) to save your photos",
    "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
    "isAccessMediaLocationEnabled": "true",
    "androidStatusBar": {
      "backgroundColor": "#C2185B",
      "translucent": false
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2e68cc1e-ba07-4332-8b0c-430523de5d53"
      }
    }
  }
}
